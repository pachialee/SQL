SELECT *
FROM superstore
ORDER BY price;

SELECT SUM(price)
FROM superstore;

SELECT MIN(price)
FROM superstore;

SELECT MAX(price)
FROM superstore;

SELECT ('Kitchen Supplies'), price
FROM superstore 
WHERE price >29.95
ORDER BY price;

--How many Cozy Throw Blanket are in stock?
SELECT SUM('Cozy Throw Blnaket')
FROM superstore;
